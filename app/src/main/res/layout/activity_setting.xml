<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <ImageButton
        android:id="@+id/backbutton_setting"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="1090dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="90dp"
        app:layout_constraintBottom_toTopOf="@+id/rootlayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_back" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="418dp"
        android:layout_height="78dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="772dp"
        android:layout_marginBottom="640dp"
        android:text="테이블 추가하기"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <LinearLayout
        android:id="@+id/rootlayout"
        android:layout_width="1100dp"
        android:layout_height="555dp"
        android:layout_marginBottom="87dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <Button
            android:id="@+id/default1"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="default"
            android:textSize="30sp" />
        <Button
            android:id="@+id/table_1"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="Table1"
            android:textSize="30sp" />

        <Button
            android:id="@+id/table_2"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="Table2"
            android:textSize="30sp" />

        <Button
            android:id="@+id/table_3"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="Table3"
            android:textSize="30sp" />

        <Button
            android:id="@+id/table_4"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="Table4"
            android:textSize="30sp" />

        <Button
            android:id="@+id/table_5"
            android:layout_width="1100dp"
            android:layout_height="111dp"
            android:gravity="center"
            android:text="Table5"
            android:textSize="30sp" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>